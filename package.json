{
	"name": "@gabrielurbina/type-guard",
	"version": "0.1.0",
	"description": "Type safety for your IO",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"prebuild": "rm -rf dist",
		"build": "tsc --project tsconfig.build.json",
		"docgen": "typedoc src/index.ts",
		"postdocgen": "mv -f docs/modules.md .",
		"prerelease": "node prerelease.js",
		"release": "cd dist && npm publish",
		"lint": "eslint src/**",
		"pretest": "tsd",
		"test": "mocha --exit",
		"pretest:cover": "tsd",
		"test:cover": "nyc mocha --exit"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/gabrielurbina/type-guard.git"
	},
	"keywords": [
		"typescript",
		"functional"
	],
	"author": "Gabriel R. Urbina R.",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/gabrielurbina/type-guard/issues"
	},
	"homepage": "https://github.com/gabrielurbina/type-guard#readme",
	"devDependencies": {
		"@types/chai": "^4.2.20",
		"@types/mocha": "^8.2.3",
		"@types/node": "^16.0.0",
		"@typescript-eslint/eslint-plugin": "^4.28.2",
		"@typescript-eslint/parser": "^4.28.2",
		"chai": "^4.3.4",
		"eslint": "^7.30.0",
		"mocha": "^9.0.2",
		"nyc": "^15.1.0",
		"prettier": "^2.3.2",
		"ts-node": "^10.0.0",
		"tsd": "^0.17.0",
		"tslib": "^2.3.0",
		"typedoc": "^0.21.2",
		"typedoc-plugin-markdown": "^3.10.2",
		"typedoc-plugin-sourcefile-url": "^1.0.6",
		"typescript": "^4.3.5"
	},
	"tsd": {
		"directory": "test",
		"compilerOptions": {
			"rootDir": "./"
		}
	}
}
