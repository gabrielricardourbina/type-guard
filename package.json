{
	"name": "@gabrielurbina/type-guard",
	"version": "0.0.5",
	"description": "Type safety for your IO",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "tsc --project tsconfig.build.json",
		"docgen": "typedoc src/index.ts",
		"postdocgen" : "mv -f docs/modules.md .",
		"prerelease": "node prerelease.js",
		"release": "npm publish dist",
		"lint": "eslint src/**",
		"pretest": "tsd",
		"test": "mocha --exit",
		"test:cover": "tsd && nyc mocha --exit"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/gabrielurbina/type-guard.git"
	},
	"keywords": [
		"typescript",
		"functional"
	],
	"author": "Gabriel R. Urbina R.",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/gabrielurbina/type-guard/issues"
	},
	"homepage": "https://github.com/gabrielurbina/type-guard#readme",
	"devDependencies": {
		"@types/chai": "^4.2.16",
		"@types/mocha": "^8.2.2",
		"@types/node": "^14.14.37",
		"@typescript-eslint/eslint-plugin": "^4.21.0",
		"@typescript-eslint/parser": "^4.21.0",
		"chai": "^4.3.4",
		"eslint": "^7.24.0",
		"esm": "^3.2.25",
		"mocha": "^8.3.2",
		"nyc": "^15.1.0",
		"prettier": "^2.2.1",
		"ts-node": "^9.1.1",
		"tsd": "^0.14.0",
		"tslib": "^2.2.0",
		"typedoc": "^0.20.36",
		"typedoc-plugin-markdown": "^3.7.2",
		"typedoc-plugin-sourcefile-url": "^1.0.6",
		"typescript": "^4.2.4"
	},
	"tsd": {
		"directory": "test",
		"compilerOptions": {
			"rootDir": "./"
		}
	}
}
